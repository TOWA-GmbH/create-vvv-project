  "bin": [
    "towa-project"
  ],
  "autoload": {
    "psr-4": {
      "Towa\\Setup\\": "src/"
    },
    "files": [
      "src/Helpers/global.php"
    ]
  },
  "require": {
    "league/climate": "^3.2",
    "oscarotero/env": "^1.1",
    "symfony/process": "^3.2",
    "tightenco/collect": "^5.6",
    "vlucas/phpdotenv": "^2.4"
  },
  "require-dev": {
    "symfony/var-dumper": "*"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
