    "bin": [
        "towa-project"
    ],
    "autoload": {
        "psr-4": {
            "Towa\\Setup\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "league/climate": "^3.2",
        "symfony/yaml": "~4.0",
        "symfony/process": "~4.0"
    },
    "require-dev": {
        "symfony/var-dumper": "*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
